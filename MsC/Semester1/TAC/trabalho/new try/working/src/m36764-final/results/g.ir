
function @g:
	t0 <- i_value 0
	fp0 <- itor t0
	@s <- r_lstore fp0
l0:
	t1 <- i_aload @n
	t2 <- i_value 1
	t3 <- i_ge t1, t2
	cjump t3, l1, l2
l1:
	fp1 <- r_lload @s
	t4 <- i_aload @n
	fp2 <- itor t4
	fp3 <- r_add fp1, fp2
	@s <- r_lstore fp3
	t5 <- i_aload @n
	t6 <- i_value 1
	t7 <- i_sub t5, t6
	@n <- i_astore t7
	jump l0
l2:
	fp4 <- r_lload @s
	r_return fp4
