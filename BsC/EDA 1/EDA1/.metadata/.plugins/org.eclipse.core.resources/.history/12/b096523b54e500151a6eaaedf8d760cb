public class Cenas {
	public static void main(String[] args) {
		System.out.println("Hello world!");

		int[] cenas = new int[10];

		for(int i = 0; i < cenas.length; i++) {
			cenas[i] = i;
			System.out.println(cenas[i]);
		}
		
		int x = 0;

		if (x == 0) {
			System.out.println("És puta");
		} else if(x == 10) {
			System.out.println("Nem sei o que és...");
		} else {
			System.out.println("Não és puta");
		}
		

		x+=10;
		System.out.println(x);

		String s = "Cenas";
		System.out.println(s);
		
		Picha picha = new Picha();
		System.out.println(picha.picha());
	}
}