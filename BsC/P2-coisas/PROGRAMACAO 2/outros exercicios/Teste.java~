public class Teste{
  public static void main(String args[]){
    Nota euro20 = new Nota(20,'€');
    System.out.println(euro20.toString());
    
    /*
    Nota euro10= new Nota(10,'€');
    Nota libra20 = new Nota(20, '£');
    ....
    */
    euro20.setValor(30);
    euro20.setMoeda('$');
    euro20.getValor();
    euro20.getMoeda();
    System.out.println(euro20.toString());
    
    Mealheiro mealheiro= new Mealheiro();
    mealheiro.tamanhoV();
    
    mealheiro.addNota(euro20);
    mealheiro.tamanhoV();
    System.out.println(mealheiro.tamanhoV());
    
    System.out.println(mealheiro.somaValor('€'));
    System.out.println(mealheiro.somaValor('£'));
    System.out.println(mealheiro.somaValor('$'));
    
  }
}