#include <stdio.h>

int my_strlen(char c[]) {
	int i = 0;

	while (c[i] != '\0')
		i++;

	return i;
}

int main() {
	char s[];
	int end;

	printf("Enter the String: \n");
	scanf("%s\n", &s);
	printf("Do you wish to continue?\n");
	scanf("%d\n", end);
	while(end == 0) {
		printf("Enter the String: \n");
		scanf("%s\n", &s);
	}



}
