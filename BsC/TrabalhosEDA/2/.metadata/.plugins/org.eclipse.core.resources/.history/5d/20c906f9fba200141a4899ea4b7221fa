public class Inputs {
	int vencedor;
	String key;
	JogoDoGalo item;
	
	public Inputs(JogoDoGalo j) {
		this.item = j;
		this.key = Builder(j.hashcode());
		vencedor = j.vencedor();
	}
	
	public String Builder(int n) {
		StringBuilder sb = new StringBuilder();
		sb.append("");
		sb.append(n);
		String s = sb.toString();
		return s;
	}

}
