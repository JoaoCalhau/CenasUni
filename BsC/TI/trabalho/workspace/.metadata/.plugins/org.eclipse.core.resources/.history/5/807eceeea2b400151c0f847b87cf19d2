		float quantBlocks = size/4;
		
		for(int i = 0; i < in.length(); i++) {
		}
		
		return null;
	}
	
	public static void main(String[] args) {
		
		String original = read(args[0]);
		
		List<Integer> l = LZW.compress(original);
		
		List<String> compressedCoded = LZW.toCode(l);
		
		String appended = LZW.append(compressedCoded);
		
		String hammingCoded = hammingCode(appended);
		
		System.out.println(hammingCoded);
	}
}
