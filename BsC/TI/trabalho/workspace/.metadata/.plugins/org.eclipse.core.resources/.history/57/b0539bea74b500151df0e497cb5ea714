import java.io.*;
import java.util.Scanner;

public class tresATres {
	
	static int total = 0;
	static int total3 = 0;

	//total A
	static int totalAAA = 0;
	static int totalAAC = 0;
	static int totalAAG = 0;
	static int totalAAT = 0;

	static int totalACA = 0;
	static int totalACC = 0;
	static int totalACG = 0;
	static int totalACT = 0;

	static int totalAGA = 0;
	static int totalAGC = 0;
	static int totalAGG = 0;
	static int totalAGT = 0;

	static int totalATA = 0;
	static int totalATC = 0;
	static int totalATG = 0;
	static int totalATT = 0;

	//totalC
	static int totalCAA = 0;
	static int totalCAC = 0;
	static int totalCAG = 0;
	static int totalCAT = 0;

	static int totalCCA = 0;
	static int totalCCC = 0;
	static int totalCCG = 0;
	static int totalCCT = 0;

	static int totalCGA = 0;
	static int totalCGC = 0;
	static int totalCGG = 0;
	static int totalCGT = 0;

	static int totalCTA = 0;
	static int totalCTC = 0;
	static int totalCTG = 0;
	static int totalCTT = 0;


	//total G
	static int totalGAA = 0;
	static int totalGAC = 0;
	static int totalGAG = 0;
	static int totalGAT = 0;

	static int totalGCA = 0;
	static int totalGCC = 0;
	static int totalGCG = 0;
	static int totalGCT = 0;

	static int totalGGA = 0;
	static int totalGGC = 0;
	static int totalGGG = 0;
	static int totalGGT = 0;

	static int totalGTA = 0;
	static int totalGTC = 0;
	static int totalGTG = 0;
	static int totalGTT = 0;


	//total T
	static int totalTAA = 0;
	static int totalTAC = 0;
	static int totalTAG = 0;
	static int totalTAT = 0;

	static int totalTCA = 0;
	static int totalTCC = 0;
	static int totalTCG = 0;
	static int totalTCT = 0;

	static int totalTGA = 0;
	static int totalTGC = 0;
	static int totalTGG = 0;
	static int totalTGT = 0;

	static int totalTTA = 0;
	static int totalTTC = 0;
	static int totalTTG = 0;
	static int totalTTT = 0;


	public static String read() {

		Scanner sc = new Scanner(System.in);
		String in = sc.nextLine();
		sc.close();

		try(BufferedReader br = new BufferedReader(new FileReader(in + ".txt"))) {

			String s = br.readLine();

			return s;

		} catch(IOException e) {
			System.out.println("Inexistent file...");
		}
		return null;
	}


	public static void calc(String s) {
		
		for(int i = 0; i < s.length()-1; i+=3) {
			if(s.charAt(i) == 'A') {
				if(s.charAt(i+1) == 'A') {
					if(s.charAt(i+2) == 'A')
						totalAAA++;
					else if(s.charAt(i+2) == 'C')
						totalAAC++;
					else if(s.charAt(i+2) == 'G')
						totalAAG++;
					else
						totalAAT++;
				} else if(s.charAt(i+1) == 'C') {
					if(s.charAt(i+2) == 'A')
						totalACA++;
					else if(s.charAt(i+2) == 'C')
						totalACC++;
					else if(s.charAt(i+2) == 'G')
						totalACG++;
					else
						totalACT++;
				} else if(s.charAt(i+1) == 'G') {
					if(s.charAt(i+2) == 'A')
						totalAGA++;
					else if(s.charAt(i+2) == 'C')
						totalAGC++;
					else if(s.charAt(i+2) == 'G')
						totalAGG++;
					else
						totalAGT++;
				} else {
					if(s.charAt(i+2) == 'A')
						totalATA++;
					else if(s.charAt(i+2) == 'C')
						totalATC++;
					else if(s.charAt(i+2) == 'G')
						totalATG++;
					else
						totalATT++;
				}
			} else if(s.charAt(i) == 'C') {
				if(s.charAt(i+1) == 'A') {
					if(s.charAt(i+2) == 'A')
						totalCAA++;
					else if(s.charAt(i+2) == 'C')
						totalCAC++;
					else if(s.charAt(i+2) == 'G')
						totalCAG++;
					else
						totalCAT++;
				} else if(s.charAt(i+1) == 'C') {
					if(s.charAt(i+2) == 'A')
						totalCCA++;
					else if(s.charAt(i+2) == 'C')
						totalCCC++;
					else if(s.charAt(i+2) == 'G')
						totalCCG++;
					else
						totalCCT++;
				} else if(s.charAt(i+1) == 'G') {
					if(s.charAt(i+2) == 'A')
						totalCGA++;
					else if(s.charAt(i+2) == 'C')
						totalCGC++;
					else if(s.charAt(i+2) == 'G')
						totalCGG++;
					else
						totalCGT++;
				} else {
					if(s.charAt(i+2) == 'A')
						totalCTA++;
					else if(s.charAt(i+2) == 'C')
						totalCTC++;
					else if(s.charAt(i+2) == 'G')
						totalCTG++;
					else
						totalCTT++;
				}
			} else if(s.charAt(i) == 'G') {
				if(s.charAt(i+1) == 'A') {
					if(s.charAt(i+2) == 'A')
						totalGAA++;
					else if(s.charAt(i+2) == 'C')
						totalGAC++;
					else if(s.charAt(i+2) == 'G')
						totalGAG++;
					else
						totalGAT++;
				} else if(s.charAt(i+1) == 'C') {
					if(s.charAt(i+2) == 'A')
						totalGCA++;
					else if(s.charAt(i+2) == 'C')
						totalGCC++;
					else if(s.charAt(i+2) == 'G')
						totalGCG++;
					else
						totalGCT++;
				} else if(s.charAt(i+1) == 'G') {
					if(s.charAt(i+2) == 'A')
						totalGGA++;
					else if(s.charAt(i+2) == 'C')
						totalGGC++;
					else if(s.charAt(i+2) == 'G')
						totalGGG++;
					else
						totalGGT++;
				} else {
					if(s.charAt(i+2) == 'A')
						totalGTA++;
					else if(s.charAt(i+2) == 'C')
						totalGTC++;
					else if(s.charAt(i+2) == 'G')
						totalGTG++;
					else
						totalGTT++;
				}
			} else {
				if(s.charAt(i+1) == 'A') {
					if(s.charAt(i+2) == 'A')
						totalTAA++;
					else if(s.charAt(i+2) == 'C')
						totalTAC++;
					else if(s.charAt(i+2) == 'G')
						totalTAG++;
					else
						totalTAT++;
				} else if(s.charAt(i+1) == 'C') {
					if(s.charAt(i+2) == 'A')
						totalTCA++;
					else if(s.charAt(i+2) == 'C')
						totalTCC++;
					else if(s.charAt(i+2) == 'G')
						totalTCG++;
					else
						totalTCT++;
				} else if(s.charAt(i+1) == 'G') {
					if(s.charAt(i+2) == 'A')
						totalTGA++;
					else if(s.charAt(i+2) == 'C')
						totalTGC++;
					else if(s.charAt(i+2) == 'G')
						totalTGG++;
					else
						totalTGT++;
				} else {
					if(s.charAt(i+2) == 'A')
						totalTTA++;
					else if(s.charAt(i+2) == 'C')
						totalTTC++;
					else if(s.charAt(i+2) == 'G')
						totalTTG++;
					else
						totalTTT++;
				}
			}
			total3++;
		}
	}
	
	public static void main(String[] args) {
		
		calc(read());
		
		total = total3*3;
		
		System.out.println("Totais :");
		System.out.println("Total de letras: " + total + "\n");
		System.out.println("Total por bloco: " + total3 + "\n");
		
		//prints A's
		System.out.println("Total AAA: " + totalAAA + ", Probabilidade: " + (float)totalAAA/total3);
		System.out.println("TOtal AAC: " + totalAAC + ", Probabilidade: " + (float)totalAAC/total3);
		System.out.println("Total AAG: " + totalAAG + ", Probabilidade: " + (float)totalAAG/total3);
		System.out.println("Total AAT: " + totalAAT + ", Probabilidade: " + (float)totalAAT/total3 + "\n");
		
		System.out.println("Total ACA: " + totalACA + ", Probabilidade: " + (float)totalACA/total3);
		System.out.println("TOtal ACC: " + totalACC + ", Probabilidade: " + (float)totalACC/total3);
		System.out.println("Total ACG: " + totalACG + ", Probabilidade: " + (float)totalACG/total3);
		System.out.println("Total ACT: " + totalACT + ", Probabilidade: " + (float)totalACT/total3 + "\n");
		
		System.out.println("Total AGA: " + totalAGA + ", Probabilidade: " + (float)totalAGA/total3);
		System.out.println("TOtal AGC: " + totalAGC + ", Probabilidade: " + (float)totalAGC/total3);
		System.out.println("Total AGG: " + totalAGG + ", Probabilidade: " + (float)totalAGG/total3);
		System.out.println("Total AGT: " + totalAGT + ", Probabilidade: " + (float)totalAGT/total3 + "\n");
		
		System.out.println("Total ATA: " + totalATA + ", Probabilidade: " + (float)totalATA/total3);
		System.out.println("TOtal ATC: " + totalATC + ", Probabilidade: " + (float)totalATC/total3);
		System.out.println("Total ATG: " + totalATG + ", Probabilidade: " + (float)totalATG/total3);
		System.out.println("Total ATT: " + totalATT + ", Probabilidade: " + (float)totalATT/total3 + "\n");
		
		//Prints C's
		System.out.println("Total CAA: " + totalCAA + ", Probabilidade: " + (float)totalCAA/total3);
		System.out.println("TOtal CAC: " + totalCAC + ", Probabilidade: " + (float)totalCAC/total3);
		System.out.println("Total CAG: " + totalCAG + ", Probabilidade: " + (float)totalCAG/total3);
		System.out.println("Total CAT: " + totalCAT + ", Probabilidade: " + (float)totalCAT/total3 + "\n");
		
		System.out.println("Total CCA: " + totalCCA + ", Probabilidade: " + (float)totalCCA/total3);
		System.out.println("TOtal CCC: " + totalCCC + ", Probabilidade: " + (float)totalCCC/total3);
		System.out.println("Total CCG: " + totalCCG + ", Probabilidade: " + (float)totalCCG/total3);
		System.out.println("Total CCT: " + totalCCT + ", Probabilidade: " + (float)totalCCT/total3 + "\n");
		
		System.out.println("Total CGA: " + totalCGA + ", Probabilidade: " + (float)totalCGA/total3);
		System.out.println("TOtal CGC: " + totalCGC + ", Probabilidade: " + (float)totalCGC/total3);
		System.out.println("Total CGG: " + totalCGG + ", Probabilidade: " + (float)totalCGG/total3);
		System.out.println("Total CGT: " + totalCGT + ", Probabilidade: " + (float)totalCGT/total3 + "\n");
		
		System.out.println("Total CTA: " + totalCTA + ", Probabilidade: " + (float)totalCTA/total3);
		System.out.println("TOtal CTC: " + totalCTC + ", Probabilidade: " + (float)totalCTC/total3);
		System.out.println("Total CTG: " + totalCTG + ", Probabilidade: " + (float)totalCTG/total3);
		System.out.println("Total CTT: " + totalCTT + ", Probabilidade: " + (float)totalCTT/total3 + "\n");
		
		
		//Prints G's
		System.out.println("Total GAA: " + totalGAA + ", Probabilidade: " + (float)totalGAA/total3);
		System.out.println("TOtal GAC: " + totalGAC + ", Probabilidade: " + (float)totalGAC/total3);
		System.out.println("Total GAG: " + totalGAG + ", Probabilidade: " + (float)totalGAG/total3);
		System.out.println("Total GAT: " + totalGAT + ", Probabilidade: " + (float)totalGAT/total3 + "\n");
		
		System.out.println("Total GCA: " + totalGCA + ", Probabilidade: " + (float)totalGCA/total3);
		System.out.println("TOtal GCC: " + totalGCC + ", Probabilidade: " + (float)totalGCC/total3);
		System.out.println("Total GCG: " + totalGCG + ", Probabilidade: " + (float)totalGCG/total3);
		System.out.println("Total GCT: " + totalGCT + ", Probabilidade: " + (float)totalGCT/total3 + "\n");
		
		System.out.println("Total GGA: " + totalGGA + ", Probabilidade: " + (float)totalGGA/total3);
		System.out.println("TOtal GGC: " + totalGGC + ", Probabilidade: " + (float)totalGGC/total3);
		System.out.println("Total GGG: " + totalGGG + ", Probabilidade: " + (float)totalGGG/total3);
		System.out.println("Total GGT: " + totalGGT + ", Probabilidade: " + (float)totalGGT/total3 + "\n");
		
		System.out.println("Total GTA: " + totalGTA + ", Probabilidade: " + (float)totalGTA/total3);
		System.out.println("TOtal GTC: " + totalGTC + ", Probabilidade: " + (float)totalGTC/total3);
		System.out.println("Total GTG: " + totalGTG + ", Probabilidade: " + (float)totalGTG/total3);
		System.out.println("Total GTT: " + totalGTT + ", Probabilidade: " + (float)totalGTT/total3 + "\n");
		
		
		//Prints T's
		System.out.println("Total TAA: " + totalTAA + ", Probabilidade: " + (float)totalTAA/total3);
		System.out.println("TOtal TAC: " + totalTAC + ", Probabilidade: " + (float)totalTAC/total3);
		System.out.println("Total TAG: " + totalTAG + ", Probabilidade: " + (float)totalTAG/total3);
		System.out.println("Total TAT: " + totalTAT + ", Probabilidade: " + (float)totalTAT/total3 + "\n");

		System.out.println("Total TCA: " + totalTCA + ", Probabilidade: " + (float)totalTCA/total3);
		System.out.println("TOtal TCC: " + totalTCC + ", Probabilidade: " + (float)totalTCC/total3);
		System.out.println("Total TCG: " + totalTCG + ", Probabilidade: " + (float)totalTCG/total3);
		System.out.println("Total TCT: " + totalTCT + ", Probabilidade: " + (float)totalTCT/total3 + "\n");
		
		System.out.println("Total TGA: " + totalTGA + ", Probabilidade: " + (float)totalTGA/total3);
		System.out.println("TOtal TGC: " + totalTGC + ", Probabilidade: " + (float)totalTGC/total3);
		System.out.println("Total TGG: " + totalTGG + ", Probabilidade: " + (float)totalTGG/total3);
		System.out.println("Total TGT: " + totalTGT + ", Probabilidade: " + (float)totalTGT/total3 + "\n");
		
		System.out.println("Total TTA: " + totalTTA + ", Probabilidade: " + (float)totalTTA/total3);
		System.out.println("TOtal TTC: " + totalTTC + ", Probabilidade: " + (float)totalTTC/total3);
		System.out.println("Total TTG: " + totalTTG + ", Probabilidade: " + (float)totalTTG/total3);
		System.out.println("Total TTT: " + totalTTT + ", Probabilidade: " + (float)totalTTT/total3 + "\n");
	}
}
